<local:ModuleBaseDlg x:Class="BrainSimulator.Modules.ModuleSpeechInDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrainSimulator.Modules"
        mc:Ignorable="d"
        Title="ModuleSpeechInDlg Module Dialog" Height="104" Width="297" Background="Gray" ShowInTaskbar="False">
    <Grid Name="theCanvas" SizeChanged="TheCanvas_SizeChanged" Margin="0,0,0,5" >
        <TextBox x:Name="TypedEntryTextBox" Canvas.Left="10" TextWrapping="Wrap" Canvas.Top="39" KeyDown="TextBox_KeyDown" Margin="0,20,0,0"/>
        <Label Content="Enable Speech Recognition" Height="29" Width="183" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="23,-6,0,0"/>
        <CheckBox x:Name="cbEnabled" Content="" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked" IsChecked="True" Height="15" VerticalAlignment="Top" HorizontalAlignment="Left" Width="15"/>
    </Grid>
</local:ModuleBaseDlg>

<local:ModuleBaseDlg x:Class="BrainSimulator.Modules.ModuleGoToDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrainSimulator.Modules"
        mc:Ignorable="d"
        Title="ModuleGoToDlg Module Dialog" Height="250" Width="400" Background="Gray" ShowInTaskbar="False">
    <Grid Name="theGrid" SizeChanged="TheCanvas_SizeChanged" >
        <ComboBox x:Name="cbThings" HorizontalAlignment="Left" Margin="10,29,0,0" VerticalAlignment="Top" Width="166" />
        <Button Content="Refresh" HorizontalAlignment="Left" Margin="268,6,0,0" VerticalAlignment="Top" Width="115" Click="ButtonRefresh_Click"/>
        <Button Content="Go Directly To Point" HorizontalAlignment="Left" Margin="268,31,0,0" VerticalAlignment="Top" Width="115" Click="ButtonGoToThing_Click"/>
        <Button Content="Route To Thing" HorizontalAlignment="Left" Margin="268,56,0,0" VerticalAlignment="Top" Width="115" Click="ButtonRouteToThing_Click"/>
    </Grid>
</local:ModuleBaseDlg>
